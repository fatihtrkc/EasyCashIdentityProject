<!DOCTYPE html>

<html>
<!-- Head -->
<head>

    <title>Easy Cash Identity - Sign Up</title>

    <!-- Meta-Tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="keywords" content="Easy Cash Identity">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //Meta-Tags -->

    <link href="~/sign_up/css/popuo-box.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Style -->
    <link rel="stylesheet" href="~/sign_up/css/style.css" type="text/css" media="all">

    <!-- Fonts -->
    <link href="//fonts.googleapis.com/css?family=Quicksand:300,400,500,700" rel="stylesheet">

    <!--Marck Script  font-family: 'Marck Script', cursive;  -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Marck+Script&display=swap" rel="stylesheet">

    <!--Comfortaa  font-family: 'Comfortaa', cursive;  -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Comfortaa:wght@300&display=swap" rel="stylesheet">

    <!--Indie Flower  font-family: 'Indie Flower', cursive;  -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Indie+Flower&display=swap" rel="stylesheet">

    <!--Gluten  font-family: 'Gluten', cursive;  -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Gluten:wght@300&display=swap" rel="stylesheet">
    <!-- //Fonts -->
</head>
<!-- //Head -->
<!-- Body -->
<body>

    <h1 style="font-family: 'Indie Flower', cursive; color:#78e08f">Easy Cash Identity Sign Up</h1>

    <div class="w3layoutscontaineragileits" style="font-family: 'Indie Flower', cursive;">
        <h2 style="text-transform:capitalize">New Customer</h2>
        <form asp-controller="SignUp" asp-action="Index" method="post">
            <input type="text" asp-for="Name" placeholder="Name" required="">
            <input type="text" asp-for="Surname" placeholder="Surname" required="">
            <input type="text" asp-for="UserName" placeholder="Username" required="">
            <input type="email" asp-for="Email" placeholder="Email" required="">
            <input type="password" asp-for="Password" placeholder="Password" required="">
            <input type="password" asp-for="ConfirmedPassword" placeholder="Password Confirm" required="">
            <div asp-validation-summary="ModelOnly"></div>
            <br /><br />
            <ul class="agileinfotickwthree">
                <li>
                    <input type="checkbox" id="isRequired" value="" required="">
                    <label for="isRequired"><span></span>I read and accepted.</label>
                </li>
            </ul>
            <div class="aitssendbuttonw3ls">
                <input type="submit" value="Sign Up">
                <p> Already have an account? <span>→</span> <a class="w3_play_icon1" href="/SignIn/Index"> Sign In</a></p>
                <div class="clear"></div>
            </div>
        </form>
    </div>

    <div class="w3footeragile">
        <p> &copy; @DateTime.Now.Year All Rights Reserved | Design by <a href="http://w3layouts.com" target="_blank">W3layouts</a></p>
    </div>


    <script type="text/javascript" src="~/sign_up/js/jquery-2.1.4.min.js"></script>
    <!-- pop-up-box-js-file -->
    <script src="~/sign_up/js/jquery.magnific-popup.js" type="text/javascript"></script>
    <!--//pop-up-box-js-file -->
    <script>
        $(document).ready(function () {
            $('.w3_play_icon,.w3_play_icon1,.w3_play_icon2').magnificPopup({
                type: 'inline',
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'my-mfp-zoom-in'
            });

        });
    </script>

</body>
<!-- //Body -->
</html>